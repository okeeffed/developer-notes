{
  "name": "dennisokeeffe-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run gen:sidebar && npm run gen:data",
    "dev": "next dev",
    "prebuild": "npm run gen:sidebar && npm run gen:data && npm run gen:folderIndex",
    "build": "next build",
    "start": "next start",
    "ts": "node --require @swc-node/register",
    "gen:data": "npm run ts -- scripts/mdx/generateMetadata.ts",
    "gen:sidebar": "npm run ts -- scripts/mdx/generateSidebar.ts",
    "gen:folderIndex": "npm run ts -- scripts/obsidian/createFolderIndexFiles.ts",
    "ext:convert": "npm run ts -- scripts/mdx/convertFileExt.ts",
    "s3": "aws s3 sync ./public/assets s3://dennisokeeffe-cdn-assets-bucket/assets --cache-control max-age=31536000",
    "imagemin": "imagemin raw-assets/* --out-dir=public/assets",
    "squoosh": "npm run ts -- scripts/images/squoosh.ts",
    "mypy": "pipenv run mypy --install-types",
    "lint-staged": "npx lint-staged"
  },
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@chakra-ui/react": "^1.6.6",
    "@choc-ui/chakra-autocomplete": "^4.13.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@heroicons/react": "^1.0.3",
    "@hookform/resolvers": "^2.7.1",
    "@mdx-js/runtime": "^1.6.22",
    "@octokit/rest": "^18.9.1",
    "airtable": "^0.11.1",
    "amplitude-js": "^8.5.0",
    "dayjs": "^1.10.6",
    "framer-motion": "^4.1.17",
    "gray-matter": "^4.0.3",
    "js-base64": "^3.6.1",
    "lodash": "^4.17.21",
    "lunr": "^2.3.9",
    "marked": "^3.0.0",
    "md5": "^2.3.0",
    "next": "latest",
    "next-mdx-remote": "^3.0.4",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.12.2",
    "react-icons": "^4.2.0",
    "react-katex": "^2.0.2",
    "react-waypoint": "^10.1.0",
    "reading-time": "^1.4.0",
    "recursive-readdir": "^2.2.2",
    "remark-unwrap-images": "^3.0.0",
    "sitemap": "^7.0.0",
    "tweetsodium": "^0.0.5",
    "yup": "^0.32.9",
    "zustand": "^3.5.12"
  },
  "devDependencies": {
    "@squoosh/lib": "^0.4.0",
    "@swc-node/register": "^1.3.4",
    "@types/amplitude-js": "^8.0.1",
    "@types/lunr": "^2.3.4",
    "@types/marked": "^2.0.4",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.18",
    "@types/recursive-readdir": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "aws-sdk": "^2.968.0",
    "axios": "^0.21.1",
    "blurhash": "^1.1.3",
    "canvas": "^2.8.0",
    "dotenv": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsdoc": "^36.0.7",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "feed": "^4.2.2",
    "fs-extra": "^10.0.0",
    "imagemin": "^8.0.1",
    "imagemin-cli": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "lint-staged": "^12.4.1",
    "merge-images": "^2.0.0",
    "mime-types": "^2.1.32",
    "node-fetch": "^2.6.1",
    "ora": "^5.4.1",
    "prettier": "^2.6.2",
    "prompts": "^2.4.2",
    "sharp": "^0.30.5",
    "type-fest": "^2.0.0",
    "typescript": "^4.3.5",
    "webpack-bundle-analyzer": "^4.5.0",
    "yargs-parser": "^20.2.9"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx|css|json|md)": [
      "npm run gen:folderIndex",
      "prettier --write"
    ]
  }
}
